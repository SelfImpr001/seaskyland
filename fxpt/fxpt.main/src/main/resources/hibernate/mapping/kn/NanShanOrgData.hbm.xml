<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 	<class name="com.cntest.remote.domain.NanShanOrgData" table="nanshan_org_data">
 		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
 		<property name="orgCode" type="java.lang.String">
 			<column name="orgCode" length="50"/>
 		</property>
 		<property name="parent" type="java.lang.String">
 			<column name="parent" length="50"/>
 		</property>
 		<property name="parents" type="java.lang.String">
 			<column name="parents" length="1000"/>
 		</property>
 		<property name="displayName" type="java.lang.String">
 			<column name="displayName" length="100"/>
 		</property>
 		<property name="name" type="java.lang.String">
 			<column name="name" length="200"/>
 		</property>
 		<property name="priority" type="java.lang.String">
 			<column name="priority" length="50"/>
 		</property>
 		
 	</class>
 </hibernate-mapping>