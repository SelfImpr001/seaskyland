<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cntest.fxpt.domain.StatisticSetting" table="kn_statisticSetting">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="stype" type="java.lang.Integer">
			<column name="sType" />
		</property>
		<property name="sname" type="java.lang.String">
			<column name="sName" length="50" />
		</property>
		<property name="svalue" type="java.lang.Integer">
			<column name="sValue" />
		</property>
		<property name="checked" type="java.lang.Integer">
			<column name="checked" />
		</property>
		<property name="status" type="java.lang.Integer">
			<column name="status" />
		</property>
	</class>
</hibernate-mapping>