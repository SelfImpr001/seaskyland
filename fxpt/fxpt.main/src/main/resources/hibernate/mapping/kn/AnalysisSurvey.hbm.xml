<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.cntest.fxpt.domain.FSDataNnalysis" table="fs_data_analysis">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="createTime" type="java.util.Date">
			<column name="create_time"  />
		</property>
		
		<property name="dataCollection" type="java.lang.String">
			<column name="data_collection"  />
		</property>
		
		<property name="monitorDate" type="java.util.Date">
			<column name="monitor_date"  />
		</property>
		
		<property name="monitorHref" type="java.lang.String">
			<column name="monitor_href" />
		</property>
		
		<property name="monitorName" type="java.lang.String">
			<column name="monitor_name"  />
		</property>
		
		<property name="monitorSchoolYear" type="java.lang.String">
			<column name="monitor_school_year"/>
		</property>
		
		<property name="monitorSemester" type="java.lang.String">
			<column name="monitor_semester"/>
		</property>
		
		<property name="monitorType" type="java.lang.String">
			<column name="monitor_type"/>
		</property>
		
		<property name="name" type="java.lang.String">
			<column name="name" />
		</property>
		
		<property name="score" type="java.lang.String">
			<column name="score" />
		</property>
		 
		<property name="specTtem" type="java.lang.String">
			<column name="spec_item"/>
		</property>
		
		<property name="status" type="java.lang.String">
			<column name="status" />
		</property>
		
		<property name="student" type="java.lang.String">
			<column name="student" />
		</property>
	</class>
</hibernate-mapping>