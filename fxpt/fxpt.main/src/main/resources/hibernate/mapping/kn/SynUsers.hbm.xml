<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cntest.fxpt.domain.SynUsers" table="kn_synusers">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="roleid" type="java.lang.Integer">
			<column name="roleid" />
		</property>
		<property name="namingrules" type="java.lang.String">
			<column name="namingrules" length="200" />
		</property>
		<property name="passwordrules" type="java.lang.String">
			<column name="passwordrules" length="10" />
		</property>
		<property name="defaultpassword" type="java.lang.String">
			<column name="defaultpassword" length="15" />
		</property>
		<property name="isSyn" type="java.lang.Integer">
			<column name="isSyn" />
		</property>
	</class>
</hibernate-mapping>