<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cntest.fxpt.domain.ApplySet" table="kn_apply_set">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="increment" />
		</id>
		<property name="systemName" type="java.lang.String">
			<column name="system_name" length="100" />
		</property>
		
		<property name="loginIcon" type="java.lang.String">
			<column name="login_icon" length="10000" />
		</property>
		<property name="handleIcon" type="java.lang.String">
			<column name="handle_icon" length="10000" />
		</property>
		<property name="status" type="java.lang.Boolean">
			<column name="status" length="1" />
		</property>
		<property name="order" type="java.lang.Integer">
			<column name="ordernum" length="10" />
		</property>
	</class>
</hibernate-mapping>